$TTL	604800

@	IN	SOA	angelrr.edu. root.angelrr.edu. (

				      1		; Serial

				 604800		; Refresh

				  86400		; Retry

				2419200		; Expire

				 604800		; default ttl

				)

				IN	NS	angelrr-linux.angelrr.edu.

angelrr-linux.angelrr.edu.	IN	A	192.168.13.10

virtual3.angelrr.edu.		IN	A	192.168.13.10

virtual4.angelrr.edu.		IN	A	192.168.13.15

otro.angelrr.edu.		IN	CNAME	virtual3.angelrr.edu.

